<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumberProgressBar">
        <attr name="nTextColor" format="color" />
        <attr name="nTextSize" format="dimension" />
        <attr name="nMax" format="integer" />
        <attr name="nCircleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RingProgressBar">
        <attr name="rRingColor" format="color" />
        <attr name="rTextColor" format="color" />
        <attr name="rTextSize" format="dimension" />
        <attr name="rCircleColor" format="color" />
        <attr name="rProgressColor" format="color" />
        <attr name="rMax" format="integer" />
        <attr name="rTextIsVisibility" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>


</resources>
